{
    "name": "cloudflare-fimg",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:astro\" \"npm run dev:worker\"",
        "dev:astro": "astro dev",
        "dev:worker": "wrangler dev",
        "dev:node": "concurrently \"npm run dev:astro\" \"tsx watch src/server.ts\"",
        "dev:next": "next dev",
        "start": "tsx src/server.ts",
        "deploy": "npm run build && wrangler deploy --minify",
        "cf-typegen": "wrangler types --env-interface CloudflareBindings",
        "build": "astro build",
        "build:next": "next build"
    },
    "dependencies": {
        "@astrojs/cloudflare": "^12.6.12",
        "@astrojs/mdx": "^4.3.13",
        "@cf-wasm/og": "^0.3.3",
        "@inox-tools/portal-gun": "^1.3.0",
        "@swup/head-plugin": "^2.3.1",
        "@types/express": "^5.0.6",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@vercel/og": "^0.8.5",
        "astro": "^5.0.0",
        "daisyui": "^5.5.8",
        "express": "^5.2.1",
        "next": "^16.0.10",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "swup": "^4.8.2"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.0.0",
        "@types/node": "^24.10.1",
        "concurrently": "^9.2.1",
        "tailwindcss": "^4.1.17",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "wrangler": "^4.53.0"
    }
}
